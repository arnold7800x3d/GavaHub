<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gavahub_nav_graph.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.arnold7800.mobileappdevproject.ui.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_businessNameRegFragment"
            app:destination="@id/businessNameRegFragment" />
        <action
            android:id="@+id/action_homeFragment_to_businessRegFragment"
            app:destination="@id/businessRegFragment" />
        <action
            android:id="@+id/action_homeFragment_to_drivingLicenseFragment"
            app:destination="@id/drivingLicenseFragment" />
        <action
            android:id="@+id/action_homeFragment_to_kraFragment"
            app:destination="@id/kraFragment" />
        <action
            android:id="@+id/action_homeFragment_to_nationalIDFragment"
            app:destination="@id/nationalIDFragment" />
        <action
            android:id="@+id/action_homeFragment_to_passportFragment"
            app:destination="@id/passportFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_logoutFragment"
            app:destination="@id/logoutFragment" />
    </fragment>
    <fragment
        android:id="@+id/businessNameRegFragment"
        android:name="com.arnold7800.mobileappdevproject.ui.fragments.BusinessNameRegFragment"

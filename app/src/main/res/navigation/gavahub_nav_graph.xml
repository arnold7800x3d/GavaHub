            android:id="@+id/action_businessRegFragment_to_businessRegFileUploadFragment"
            app:destination="@id/businessRegFileUploadFragment" />
    </fragment>
    <fragment
        android:id="@+id/businessRegPaymentFragment"
        android:name="com.arnold7800.mobileappdevproject.ui.fragments.BusinessRegPaymentFragment"
        android:label="BusinessRegPaymentFragment" />

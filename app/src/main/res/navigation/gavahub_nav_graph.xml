        android:id="@+id/logoutFragment"
        android:name="com.arnold7800.mobileappdevproject.ui.fragments.LogoutFragment"
        android:label="LogoutFragment" >
        <action
            android:id="@+id/action_logoutFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.arnold7800.mobileappdevproject.MainActivity"
        android:label="MainActivity" />
